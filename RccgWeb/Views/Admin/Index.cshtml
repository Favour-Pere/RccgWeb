@model RccgWeb.ViewModel.AdminViewModel

@{
    ViewData["Title"] = "Administrative Page";
}

<div>
    <h2>Welcome Admin</h2>

    @foreach (var user in Model.Users)
    {
        <div>
            @user.Firstname
            @user.Lastname
        </div>
    }

    @foreach (var church in Model.Churches)
    {
        <div>
            @church.ChurchName
            @church.ChurchLocation
        </div>
    }
</div>